int lengthOfLastWord(string s) 
{
    int count=0;
    int n=s.size();
    int i, j;
    for(i=n-1; i>=0; i--)
    {
        if(s[i]!=' ')
        {
            break;
        }
    }
    for(j=i; j>=0; j--)
    {
        if(s[j]==' ')
        {
            break;
        }
    }
    return (i-j);
}
